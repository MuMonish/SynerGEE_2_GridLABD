#include "Regulators_TUR115_Closed.glm";
#include "Switches_TUR115_Closed.glm";
#include "Transformers_TUR115_Closed.glm";


object collector {
	 name loss_OH;
	 group class=overhead_line;
	 property sum(power_losses.real);
	 file "OH_lines_losses.csv";
}


object voltdump {
	 filename output_voltage.csv;
	 mode polar;
}


object currdump {
	 filename output_current.csv;
	 mode polar;
}


